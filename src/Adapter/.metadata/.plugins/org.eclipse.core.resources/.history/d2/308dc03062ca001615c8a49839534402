#include "chip.h"
#include "timer.h"


/* The instance function of  the Timer singleton */
Timer * Timer::Instance(Timer_T timer_t)
{
	static Timer timer0(Timer0);
    static Timer timer0(Timer1);
    static Timer timer0(Timer2);

}

/* Constructor */
Timer::Timer(Timer_T tiemr_t)
{
	switch(tiemr_t)
	{
	case Timer0:
		timer_ = LPC_MRT_CH0;
		break;
	case Timer1:
		timer = LPC_MRT_CH1;
	case Timer2:


	}
}







